[user]
	name = rei-m
	email = star-oceans@abelia.ocn.ne.jp
[credential]
	helper = osxkeychain
[alias]
    st = status
    cm = commit
    cme = commit --allow-empty
    co = checkout
    graph = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
[core]
    excludesfile = ~/.gitignore
